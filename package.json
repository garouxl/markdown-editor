{
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "webpack-dashboard -- node server.js",
    "test": "jest --coverage --no-cache",
    "test:watch": "yarn test -- --watch",
    "lint": "standard --parser babel-eslint --verbose | snazzy",
    "lint:watch": "gulp",
    "lint:fix": "standard --fix",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --progress --config webpack/prod.config -p",
    "build:analyzer": "cross-env ANALYZER=true yarn build",
    "storybook": "start-storybook -p 6006 -c storybook",
    "storybook:build": "cross-env NODE_ENV=production BABEL_ENV=production build-storybook -c storybook",
    "update-packages": "node scripts/update.js"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.30.1",
    "babel-core": "^6.18.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "0.1.16",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "^6.0.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.24.1",
    "jest-cli": "^17.0.3",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "snazzy": "^5.0.0",
    "standard": "^14.3.4",
    "standard-loader": "^5.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^4.1.0",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "highlight.js": "^9.10.0",
    "marked": "^1.1.1",
    "node-uuid": "^1.4.8",
    "normalize.css": "^8.0.1",
    "preact": "^7.2.1",
    "preact-compat": "^3.14.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack-bundle-analyzer": "^3.8.0"
